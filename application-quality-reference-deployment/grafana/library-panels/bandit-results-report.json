        "hide": false,
        "rawQuery": true,
        "rawSql": "SELECT\n  elem ->> 'filename' AS \"File\",\n  elem ->> 'test_id' AS \"Code\",\n  elem ->> 'more_info' AS \"link\",\n  elem -> 'issue_cwe' ->> 'id' AS \"Weakness (CWE)\",\n  elem -> 'issue_cwe' ->> 'link' AS \"issue_cwe_link\",\n  -- (elem -> 'line_number') :: int AS \"Line\",\n  -- (elem -> 'col_offset') :: int AS \"Column\",\n  'L' || (elem ->> 'line_number') || 'C' || (elem ->> 'col_offset') AS \"Location\",\n  elem ->> 'issue_severity' AS \"Severity\",\n  elem ->> 'issue_confidence' AS \"Confidence\",\n  elem ->> 'issue_text' AS \"Message\"\nFROM\n  (\n    SELECT\n      jsonb_array_elements(output :: jsonb -> 'results') AS elem\n    FROM\n      reports_admin\n    WHERE\n      id = ${report_id}\n  ) AS sub;",
