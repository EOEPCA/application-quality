        "datasource": {
          "type": "grafana-postgresql-datasource",
          "uid": "application-quality-db"
        },
        "editorMode": "code",
        "format": "table",
        "hide": false,
        "rawQuery": true,
        "rawSql": "SELECT\n  (t.row_data :: jsonb) ->> 'filename' AS \"Path to File\",\n  (t.row_data :: jsonb) ->> 'text' AS \"Message\",\n  (t.row_data :: jsonb) ->> 'code' AS \"Code\",\n  'L' || ((t.row_data :: jsonb) ->> 'line_number') || 'C' || ((t.row_data :: jsonb) ->> 'column_number') AS \"Location\",\n  -- (t.row_data :: jsonb) ->> 'line_number' AS Line,\n  -- (t.row_data :: jsonb) ->> 'column_number' AS Column,\n  (t.row_data :: jsonb) ->> 'physical_line' AS \"Source Code\"\n  -- t.row_data AS val\nFROM (\n  SELECT\n  unnest(\n  ARRAY(\n    SELECT\n      ARRAY[val]\n    FROM\n      jsonb_each(report.output::jsonb) as t,\n      jsonb_array_elements_text(t.value) AS val\n  )\n  ) AS row_data\nFROM reports_admin as report\nWHERE report.id = ${report_id}\n) AS t;",
        "refId": "A",
        "sql": {
          "columns": [
            {
              "parameters": [],
              "type": "function"
            }
          ],
          "groupBy": [
            {
              "property": {
                "type": "string"
              },
              "type": "groupBy"
            }
          ],
          "limit": 50
        }
      }
    ],
    "title": "Flake8 Report",
    "type": "table"
  }
