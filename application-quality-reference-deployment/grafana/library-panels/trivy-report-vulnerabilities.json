        "rawQuery": true,
        "rawSql": "SELECT\n  -- vulnerabilities ->> 'PkgID' AS \"Package\",\n  vulnerabilities ->> 'PkgName' AS \"Package\",\n  vulnerabilities ->> 'Severity' AS \"Severity\",\n  vulnerabilities ->> 'Status' AS \"Status\",\n  vulnerabilities ->> 'InstalledVersion' AS \"Installed Version\",\n  vulnerabilities ->> 'FixedVersion' AS \"Fixed Version\",\n  vulnerabilities ->> 'VulnerabilityID' AS \"Vulnerability ID\",\n  vulnerabilities ->> 'PrimaryURL' AS \"Link\",\n  vulnerabilities ->> 'Title' AS \"Description\"\n  -- vulnerabilities ->> 'Description' AS \"Description\"\nFROM\n  (\n    SELECT\n      jsonb_array_elements(output :: jsonb -> 'Results' -> 0 -> 'Vulnerabilities') AS vulnerabilities\n    FROM\n      reports_admin\n    WHERE\n      id = ${report_id}\n  ) AS sub;",
