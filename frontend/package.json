{
  "name": "application-quality",
  "version": "0.0.0",
  "private": true,
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "vite build",
    "build:prod": "vite build --mode production",
    "preview": "vite preview",
    "_preview": "vite preview --host $VITE_API_HOST --port $VITE_API_PORT",
    "lint": "eslint . --fix-dry-run --format stylish",
    "lint:write": "eslint . --fix",
    "format": "prettier --single-quote src/",
    "format:write": "prettier --single-quote --write src/",
    "test": "vitest run",
    "test:list": "vitest list",
    "test:watch": "vitest watch",
    "test:coverage": "vitest run --coverage"
  },
  "dependencies": {
    "@mdi/font": "^7.4.47",
    "axios": "^1.7.7",
    "pinia": "^2.2.6",
    "vue": "^3.5.12",
    "vue-json-to-html-table": "^0.0.7",
    "vue-router": "^4.4.5",
    "vuetify": "^3.7.4",
    "vite": "^5.4.10",
    "@vitejs/plugin-vue": "^5.1.4"
  },
  "devDependencies": {
    "@eslint/js": "^9.14.0",
    "@vue/eslint-config-prettier": "^10.1.0",
    "@vue/test-utils": "^2.4.6",
    "eslint": "^9.14.0",
    "eslint-plugin-vue": "^9.30.0",
    "jsdom": "^26.0.0",
    "prettier": "^3.3.3",
    "resize-observer-polyfill": "^1.5.1",
    "vitest": "^3.0.6"
  }
}
